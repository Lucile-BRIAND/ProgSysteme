<Window x:Class="ConsoleDeportee.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConsoleDeportee"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ProgressBar x:Name="pbstatus1" HorizontalAlignment="Left" Height="31" Margin="215,0,0,0" VerticalAlignment="Center" Width="380" ValueChanged="pbstatus1_ValueChanged"/>
        <DataGrid  Name="executeJobDataGrid" HorizontalAlignment="Left" Height="154" Margin="171,25,0,0" VerticalAlignment="Top" Width="472" SelectionChanged="DataGrid_SelectionChanged"/>
        <Button Content="Pause" HorizontalAlignment="Left" Margin="370,268,0,0" VerticalAlignment="Top" Height="25" Width="56" Click="ButtonPauseJob_Click"/>
        <Button Content="Resume" HorizontalAlignment="Left" Height="24" Margin="458,269,0,0" VerticalAlignment="Top" Width="65" Click="ButtonStopJob_Click"/>
        <Label x:Name="lb_etat_prog_server" Content="" HorizontalAlignment="Left" Margin="305,322,0,0" VerticalAlignment="Top" Height="58" Width="220" FontSize="24"/>
        <Button Content="Stop" HorizontalAlignment="Left" Height="25" Margin="279,268,0,0" VerticalAlignment="Top" Width="52"/>

    </Grid>
</Window>
